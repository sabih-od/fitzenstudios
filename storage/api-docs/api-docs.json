                        "description": "Options: ['Asia/Kolkata', 'America/Los_Angeles', 'America/Chicago', 'America/New_York']"
                    }
                ],
                "summary": "Register a new user.",
                "operationId": "route-register",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "example": {
                                    "first_name": "Cedric",
                                    "last_name": "Maya",
                                    "email": "asd441@mailinator.com",
                                    "password": "admin!@#",
                                    "phone": "1234",
                                    "time_zone": "Asia/Kolkata"
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "success",
                        "content": {
                            "application/json": {
                            }
                        }
                    }
                }
            }
        },
